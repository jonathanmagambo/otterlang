use fmt

fn main():
    # Test regular \n escape sequence (already supported)
    fmt.println("Line 1\nLine 2\nLine 3")
    
    # Test multi-line string with actual newlines
    let multiline = """
This is a
multi-line string
with actual newlines!
"""
    fmt.println(multiline)
    
    # Test multi-line string with quotes inside
    let with_quotes = """
This string has "quotes" inside
and it works fine!
"""
    fmt.println(with_quotes)

